<%= link_to "Wikinimous", articles_path %>
<%= link_to "New article", new_article_path %>

<h1><%= @article.title %></h1>
<%= link_to article_path(@article), method: :delete do %>
  <i class="fa fa-trash-o"></i>
  <% end %>

<%= link_to edit_article_path(@article) do %>
  <i class="fa fa-pencil"></i>
  <% end %>

<p><%= @article.content %></p>
